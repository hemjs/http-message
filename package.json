{
  "name": "@hemjs/http-message",
  "version": "0.3.1",
  "description": "Common classes and constants for HTTP messages",
  "author": "Augustus Kamau",
  "license": "MIT",
  "keywords": [
    "hemjs",
    "http",
    "http-message"
  ],
  "scripts": {
    "prebuild": "rimraf -rf dist",
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prerelease": "npm run prebuild && npm run build",
    "release": "release-it"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@types/express": "4.17.14",
    "@types/node": "18.11.6",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "release-it": "15.5.0",
    "rimraf": "3.0.2",
    "typescript": "4.8.4"
  },
  "homepage": "https://github.com/hemjs/http-message#readme",
  "bugs": {
    "url": "https://github.com/hemjs/http-message/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemjs/http-message.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
